{
  "name": "expressjs-typescript-starter",
  "version": "1.0.0",
  "description": "Starter repo for ExpressJS API with Typescript\n\n",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --respawn src/index.ts",
    "debug": "ts-node-dev --inspect --exit-child --respawn src/index.ts",
    "lint": "eslint src/**/*.ts",
    "migrate": "sh ./setup/mongo/migrate-mongo.sh $1 $2",
    "build": "rm -rf dist && tsc -p ./tsconfig.json",
    "postbuild": "cp -r src/resources/ dist/resources",
    "test": "reset && NODE_ENV=test jest --detectOpenHandles --silent=false --verbose --forceExit ",
    "checkTS": "yarn build && rm -rf -r ./dist",
    "generate": "node ./tools/generate.js"
  },
  "author": "BLUU",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.11",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "generate-template-files": "^3.0.0",
    "helmet": "^4.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "migrate-mongo": "^8.1.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.8",
    "mongoose-aggregate-paginate-v2": "^1.0.2",
    "mongoose-paginate-v2": "^1.3.6",
    "mongoose-sequence": "^5.2.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.6.3",
    "nodemailer-express-handlebars": "^4.0.0",
    "nodemon": "^2.0.12",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "validator": "^12.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/mongoose": "^5.10.3",
    "@types/mongoose-aggregate-paginate-v2": "^1.0.1",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/mongoose-sequence": "^3.0.4",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-express-handlebars": "^4.0.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "apidoc": "^0.17.7",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.0",
    "prettier": "2.0.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
